{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "customer_id": {
      "type": "integer",
      "unique": true,
      "required": true
    },
    "first_name": {
      "type": "string",
      "required": true,
      "regex": "^[A-Za-z]{2,30}$",
      "default": "Test1"
    },
    "last_name": {
      "type": "string",
      "required": true,
      "regex": "^[A-Za-z]{2,30}$",
      "default": "test"
    },
    "nic": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 12,
      "default": "200307911760"
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "membership": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::membership.membership",
      "mappedBy": "customer"
    },
    "booking": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::booking.booking",
      "inversedBy": "customer"
    },
    "mobile_no": {
      "type": "string",
      "required": true,
      "maxLength": 10,
      "default": "0777777777",
      "regex": "^0[1-9][0-9]{8}$"
    }
  }
}
